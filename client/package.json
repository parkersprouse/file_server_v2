{
  "name": "web_file_browser_client",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "browserslist": "since 2018, > 0.3%, not dead",
  "engines": {
    "node": "^22",
    "pnpm": "^10"
  },
  "scripts": {
    "start": "pnpm run dev",
    "dev": "vite",
    "prebuild": "pnpm run clean:dist",
    "build": "run-p check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "check": "vue-tsc -b",
    "lint": "run-s --continue-on-error lint:css lint:js",
    "lint:fix": "run-s --continue-on-error lint:css:fix lint:js:fix",
    "lint:css": "stylelint -f verbose \"**/*.{css,vue}\"",
    "lint:css:fix": "stylelint -f verbose --fix \"**/*.{css,vue}\"",
    "lint:css:ci": "stylelint --custom-formatter \"@csstools/stylelint-formatter-github\" \"**/*.{css,vue}\"",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "clean:dist": "rimraf -I dist",
    "preclean": "pnpm run clean:dist",
    "clean": "rimraf -I -g \"**/node_modules\"",
    "clean:lockfile": "rimraf -I -g \"**/pnpm-lock.yaml\"",
    "clean:deep": "run-s clean:lockfile clean",
    "refresh": "pnpm run clean && pnpm install",
    "refresh:deep": "pnpm run clean:deep && pnpm install",
    "component:add": "pnpm exec shadcn-vue add"
  },
  "dependencies": {
    "@fontsource-variable/inter": "^5.2.8",
    "@fontsource-variable/source-code-pro": "^5.2.7",
    "@tailwindcss/vite": "^4.1.14",
    "@vueuse/core": "^13.9.0",
    "axios": "^1.12.2",
    "caniuse-lite": "^1.0.30001751",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.18",
    "emittery": "^1.2.0",
    "media-chrome": "^4.14.0",
    "pinia": "^3.0.3",
    "reka-ui": "^2.5.1",
    "sanitize.css": "^13.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.14",
    "tw-animate-css": "^1.4.0",
    "ua-parser-js": "^2.0.6",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@csstools/stylelint-formatter-github": "^1.0.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@iconify-json/ph": "^1.2.2",
    "@iconify-json/ri": "^1.2.6",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@stylistic/stylelint-plugin": "^4.0.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/caniuse-lite": "^1.0.5",
    "@types/node": "^22.16.5",
    "@types/prismjs": "^1.26.5",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.38.0",
    "eslint-import-resolver-custom-alias": "^1.3.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-unicorn": "^61.0.2",
    "eslint-plugin-vue": "~10.5.1",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "lightningcss": "^1.30.2",
    "npm-run-all2": "^8.0.4",
    "rimraf": "^6.0.1",
    "shadcn-vue": "^2.3.2",
    "stylelint": "^16.25.0",
    "stylelint-config-clean-order": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-plugin-defensive-css": "^1.0.4",
    "stylelint-use-nesting": "^6.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "unplugin-icons": "^22.4.2",
    "unplugin-vue-components": "^29.1.0",
    "vite": "^7.1.11",
    "vite-plugin-toml": "^0.8.5",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vue-tsc": "^3.1.1"
  },
  "pnpm": {
    "updateConfig": {
      "ignoreDependencies": [
        "@types/node"
      ]
    },
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild",
      "unrs-resolver",
      "vue-demi"
    ]
  }
}

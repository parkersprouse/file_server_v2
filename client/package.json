{
  "name": "web_file_browser_client",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "browserslist": "since 2018, > 0.3%, not dead",
  "engines": {
    "node": "^22",
    "pnpm": "^10"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": "22.15.0",
      "onFail": "error"
    },
    "packageManager": {
      "name": "pnpm",
      "version": "10.14.0",
      "onFail": "error"
    }
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748",
  "scripts": {
    "start": "pnpm run dev",
    "dev": "vite",
    "prebuild": "pnpm run clean:dist",
    "build": "run-p check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "check": "vue-tsc -b",
    "lint": "run-s --continue-on-error lint:css lint:js",
    "lint:fix": "run-s --continue-on-error lint:css:fix lint:js:fix",
    "lint:css": "stylelint -f verbose \"**/*.{css,vue}\"",
    "lint:css:fix": "stylelint -f verbose --fix \"**/*.{css,vue}\"",
    "lint:css:ci": "stylelint --custom-formatter \"@csstools/stylelint-formatter-github\" \"**/*.{css,vue}\"",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "clean:dist": "rimraf -I dist",
    "preclean": "pnpm run clean:dist",
    "clean": "rimraf -I -g \"**/node_modules\"",
    "clean:all": "rimraf -I -g \"**/pnpm-lock.yaml\" && pnpm run clean",
    "component:add": "pnpm exec shadcn-vue add"
  },
  "dependencies": {
    "@fontsource-variable/inter": "^5.2.6",
    "@tailwindcss/vite": "^4.1.11",
    "@vueuse/core": "^13.6.0",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "lucide-vue-next": "^0.532.0",
    "pinia": "^3.0.3",
    "reka-ui": "^2.4.0",
    "sanitize.css": "^13.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.6",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@csstools/stylelint-formatter-github": "^1.0.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@iconify-json/ph": "^1.2.2",
    "@stylistic/eslint-plugin": "^5.2.2",
    "@stylistic/stylelint-plugin": "^4.0.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.16.5",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.32.0",
    "eslint-import-resolver-custom-alias": "^1.3.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-n": "^17.21.0",
    "eslint-plugin-unicorn": "^60.0.0",
    "eslint-plugin-vue": "~10.3.0",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "lightningcss": "^1.30.1",
    "npm-run-all2": "^8.0.4",
    "rimraf": "^6.0.1",
    "shadcn-vue": "^2.2.0",
    "stylelint": "^16.22.0",
    "stylelint-config-clean-order": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-plugin-defensive-css": "^1.0.4",
    "stylelint-use-nesting": "^6.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "unplugin-icons": "^22.2.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vue-tsc": "^3.0.4"
  },
  "pnpm": {
    "updateConfig": {
      "ignoreDependencies": [
        "@types/node"
      ]
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "vue-demi"
    ]
  }
}
